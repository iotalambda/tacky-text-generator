# Environment variables for gif-text-animator
# Copy this file to .env.local and modify values as needed

# Set to 'true' to see the calibration process visually (camera backing off, animation cycling)
# Set to 'false' or remove to hide calibration (shows "Generating..." overlay instead)
VITE_SHOW_CALIBRATION=false

# Set to 'true' to show the calibrated bounding box as a gray overlay on the scene
VITE_SHOW_BOUNDING_BOX=false
